{
    "dashboard": {
        "id": null,
        "title": "Gateway Auth Test",
        "tags": [
            "k6",
            "auth0",
            "gateway",
            "performance"
        ],
        "timezone": "browser",
        "panels": [
            {
                "id": 1,
                "title": "üîê Auth Performance",
                "type": "stat",
                "targets": [
                    {
                        "datasource": "InfluxDB-K6",
                        "rawQuery": true,
                        "query": "SELECT mean(\"value\") FROM \"checks\""
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "red",
                                    "value": 0
                                },
                                {
                                    "color": "yellow",
                                    "value": 0.95
                                },
                                {
                                    "color": "green",
                                    "value": 0.99
                                }
                            ]
                        },
                        "unit": "percentunit"
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 0,
                    "y": 0
                }
            },
            {
                "id": 2,
                "title": "üöÄ Request Rate",
                "type": "stat",
                "targets": [
                    {
                        "datasource": "InfluxDB-K6",
                        "rawQuery": true,
                        "query": "SELECT mean(\"value\") FROM \"http_reqs\""
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "palette-classic"
                        },
                        "unit": "reqps"
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 6,
                    "y": 0
                }
            },
            {
                "id": 3,
                "title": "‚è±Ô∏è Response Time",
                "type": "timeseries",
                "targets": [
                    {
                        "datasource": "InfluxDB-K6",
                        "rawQuery": true,
                        "query": "SELECT mean(\"value\") FROM \"http_req_duration\" GROUP BY time(1m) fill(null)"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "palette-classic"
                        },
                        "unit": "ms"
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 0
                }
            },
            {
                "id": 4,
                "title": "‚úÖ Success Rate",
                "type": "timeseries",
                "targets": [
                    {
                        "datasource": "InfluxDB-K6",
                        "rawQuery": true,
                        "query": "SELECT mean(\"value\") FROM \"checks\" GROUP BY time(1m) fill(null)"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "red",
                                    "value": 0
                                },
                                {
                                    "color": "yellow",
                                    "value": 0.95
                                },
                                {
                                    "color": "green",
                                    "value": 0.99
                                }
                            ]
                        },
                        "unit": "percentunit",
                        "min": 0,
                        "max": 1
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 24,
                    "x": 0,
                    "y": 8
                }
            },
            {
                "id": 5,
                "title": "üìä Test Summary",
                "type": "table",
                "targets": [
                    {
                        "datasource": "InfluxDB-K6",
                        "rawQuery": true,
                        "query": "SELECT * FROM \"iterations\" ORDER BY time DESC LIMIT 100"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 24,
                    "x": 0,
                    "y": 16
                }
            }
        ],
        "time": {
            "from": "now-30d",
            "to": "now"
        },
        "refresh": "5s",
        "schemaVersion": 30,
        "version": 1,
        "uid": "gateway-auth-test",
        "templating": {
            "list": []
        }
    }
}